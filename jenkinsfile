pipeline{
    agent any

    stages{
        stage('Git Checkout'){
            steps{
                git branch: 'main', url: 'https://github.com/durgeswararao/demo-counter-app.git'
            }
        }
        stage('UNIT Testing'){
            steps{
                 withMaven(maven: 'mvn') {
            sh "mvn test"
        }
            }
       
    }
        
    }
}